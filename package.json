{
  "name": "vuepress-plugin-easy-sidebar",
  "version": "1.0.0",
  "description": "a vuepress plugin for generate sidebar",
  "keywords": [
    "vuepress",
    "plugin",
    "auto",
    "sidebar"
  ],
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
